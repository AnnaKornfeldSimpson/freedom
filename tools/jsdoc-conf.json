{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["src/", "interface/", "providers/"],
        "exclude": ["src/util/"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "recurse": true,
        "private": true
    },
    "plugins": [],
    "templates": {
        "cleverLinks": true,
        "default": {
            "outputSourceFiles": true
        }
    },
    "jsVersion": 180
}
